package quiz03;

public class Student {
	String name;
	int kor, eng, mat;

	void set(String n, int k, int e, int m) {
		String name = n;
		int kor = k;
		int eng = e;
		int mat = m;
	}
	
	double getAverage() {
		return (kor + eng + mat) / 3.0;
	}
	
	String getPass() {
		return (((kor + eng + mat) / 3.0) > 60) ? "합격" : "불합격";
		
	}

}
