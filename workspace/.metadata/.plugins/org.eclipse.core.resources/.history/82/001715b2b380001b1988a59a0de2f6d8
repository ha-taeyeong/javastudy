package ex02_2d_array;

import java.util.Scanner;

public class Quiz05 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String[][] dict = {
				{ "봄", "spring" },
				{ "여름", "summer" },
				{ "가을", "fall" },
				{ "겨울", "winter" }
		};
		
		int wrong = 0; // 틀린 문제의 갯수
		
		for(int i=0; i < dict.length; i++) { // i는 계절
			for(int j=0; j < dict[i].length; j++) { // j는 한글 영어
				System.out.println(dict[i]+ "을 영어로 하면?");
				String a = sc.next();
				
			}
		}
		
		

	}

}
