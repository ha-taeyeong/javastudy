package ex03_overloading;

public class Rectangle {
	
	//field
	int width; // 너비
	int height; // 높이
	
	//method
	void set(int w, int h) {
		if(w > 0) {
			width = w;
		}
		if(h > 0) {
		height = h;
		}
	}
	
	void set(int n) {
		if(n > 0) {
			width = n;
			height = n;
		}
	}
	
	int getArea() {
		return width * height;
	}
}
