package ex01_random;

import java.util.Scanner;

public class Quiz04 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String[] gababo = { "가위", "바위", "보" };
		
		
		
		while(true) {
			String Player = sc.next();
			int computer = (int)(Math.random() * 3);
			System.out.println(gababo[computer]);
			
		}
		

	}

}
