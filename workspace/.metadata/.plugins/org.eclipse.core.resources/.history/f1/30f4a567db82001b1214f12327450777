package quiz01;

public class Watch {
	
	private int addHour;
	private int addMinute;
	private int addSecond;
	
	public void Watch(int n) {
		if(n >= 25) {
			addHour += 1;
			return;
		}else if(n >= 61) {
			addMinute += 1;
			return;
		}
		n++;
	}

	public int getAddHour() {
		return addHour;
	}

	public void setAddHour(int addHour) {
		this.addHour = addHour;
	}

	public int getAddMinute() {
		return addMinute;
	}

	public void setAddMinute(int addMinute) {
		this.addMinute = addMinute;
	}

	public int getAddSecond() {
		return addSecond;
	}

	public void setAddSecond(int addSecond) {
		this.addSecond = addSecond;
	}
	
}
