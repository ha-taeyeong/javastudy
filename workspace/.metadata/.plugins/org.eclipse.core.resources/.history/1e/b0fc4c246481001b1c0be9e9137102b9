package ex02_String;

import java.util.Scanner;

public class Quiz02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("입력 >>> ");
		int n = sc.nextInt();
		
		if(n == (double)n) {
			System.out.println(n + "실수입니다.");
		}else {
			System.out.println(n + "정수입니다.");
		}

	}

}
