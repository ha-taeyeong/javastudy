package ex02_2d_array;

public class Quiz01 {

	public static void main(String[] args) {
		
		// 5층 2호
		int[][] apt = {
				{ 1, 2 },
				{ 5, 2 },
				{ 3, 3 },
				{ 1, 4 },
				{ 5, 2 },
		};
		
		for(int i=0; i < apt.length; i++) {
			for( int j=0; j < apt[i].length; i++) {
				System.out.println((i + 1) +"층에"+ j +"명이 삽니다.");
			}
			System.out.println();
		}

	}

}
