package ex01_random;

public class Quiz03 {

	public static void main(String[] args) {
		String[] yut = { "도", "개", "걸", "윷", "모" };
		int move = 0; // 이동횟수
		
		while(true) {
			
			// 윷을 프로그램이 던집니다.
			int idx = (int)(Math.random() * 5);
			System.out.print("\"" + yut[idx] + "\"");
			
		}

	}

}
