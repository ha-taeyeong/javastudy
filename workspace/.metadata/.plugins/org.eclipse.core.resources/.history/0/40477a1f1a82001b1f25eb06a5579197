package ex02_method;

public class Person {
	
	//field
	String name;
	int age;
	char gender;
	boolean isKorean;
	
	//method
	// 1. 결과타입 : 결과값이 없다.(void)
	// 2. 메소드명 : set
	// 3. 매개변수 : String n, int a, char g, boolean isK
	// 4. 역할 : 전달되는 인수를 필드에 전달합니다.
	void set(String n, int a, char g, boolean isK) {
		name = n;
		age = a;
		gender = g;
		isKorean = isK;
		
	}
}
