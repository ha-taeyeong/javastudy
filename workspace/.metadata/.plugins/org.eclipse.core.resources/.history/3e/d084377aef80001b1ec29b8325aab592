package ex02_2d_array;

public class Quiz02 {

	public static void main(String[] args) {
		int[][] gugudan = new int[8][9];
		for (int i = 0; i < gugudan.length; i++) {
			for(int j = 0; j < gugudan.length; j++) {
				gugudan[i][j] = (i+2) * (j+1);
				System.out.print(gugudan[i][j] + "\t");
			}
			System.out.println();
		}

	}

}
