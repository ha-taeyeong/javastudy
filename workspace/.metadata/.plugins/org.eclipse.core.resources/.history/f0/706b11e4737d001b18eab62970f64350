package ex01_branch;

import java.util.Scanner;

public class Quiz04 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("입력 >> ");
		int a = sc.nextInt();
		
		switch(a) {
		case 1:
			System.out.println("기승전결");
			break;
		case 2:
			System.out.println("승전결");
			break;
		case 3:
			System.out.println("전력");
			break;
		case 4:
			System.out.println("결");
			break;
		}

	}

}
