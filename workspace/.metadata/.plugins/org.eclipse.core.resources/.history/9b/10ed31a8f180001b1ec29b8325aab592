package ex02_2d_array;

public class Quiz03 {

	public static void main(String[] args) {
		String[][] matrix = new String[10][10];
		for (int i = 0; i < matrix.length; i++) {
			for (int j = 0; j < matrix[i].length; j++) {
				int n = (i+10) * (j+1);
				
				int units = i%10;
				int tens = i/10;
				
				if(units % 3 == 0) {
					System.out.println("짝");
				}else if(tens % 3 == 0) {
					System.out.println("짝");
				}
				System.out.println(matrix[i][j]);
				
			
			}
			
		}
		
		
		
		
	}
	
}
