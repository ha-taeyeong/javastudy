package ex01_array;

import java.util.Scanner;

public class Quiz01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String[] arr = new String[3];
		
		String total = "";
		for(int i = 0; i < arr.length; i++) {
			System.out.print("입력 >>> ");
			arr[i] = sc.next();
			total += arr[i];
		}
		System.out.println(total + "\t");
	}
}
