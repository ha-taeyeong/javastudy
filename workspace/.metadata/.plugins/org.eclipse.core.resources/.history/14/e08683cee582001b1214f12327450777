package quiz03;

public class Singer {
	
	//field
	private String name;
	private Song song; // private Song[] songs;
	
	//constructor
	public Singer(String name) {
		this.name = name;
	}

	public void setSong(Song song) {
		this.song = song;
	}


	public void info() {
		System.out.println(name);
		System.out.println(song);
	}
}
