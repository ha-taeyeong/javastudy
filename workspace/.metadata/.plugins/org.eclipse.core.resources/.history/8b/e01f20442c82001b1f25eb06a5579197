package quiz03;

public class Student {
	String name;
	int kor;
	int eng;
	int math;
	double Average;
	void set(String n, int k, int e, int m) {
		String name = n;
		int kor = k;
		int eng = e;
		int math = m;
	}
	
	double getAverage() {
		double Average = (kor + eng + math) / 3;
		return Average;
	}
	
	String getPass() {
		return (Average > 60) ? "합격" : "불합격";
		
	}

}
