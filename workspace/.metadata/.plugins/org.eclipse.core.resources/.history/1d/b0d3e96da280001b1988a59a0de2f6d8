package ex02_2d_array;

public class Quiz02 {

	public static void main(String[] args) {
		int[][] gugudan = new int[8][9];

		for (int i = 0; i < gugudan.length; i++) { // i는 dan과 관련
			for( int j = 0; j < gugudan[i].length; j++) { // j는 n과 관련
				gugudan[i][j] = (i+2)*j;
				System.out.print( gugudan[i][j] + " ");
			}
		System.out.println();
		}

	}

}
