package ex02_2d_array;

public class Quiz02 {

	public static void main(String[] args) {
		int[][] gugu = new int[9][9];
		for (int i = 0; i < gugu.length; i++) {
			gugu[i]=i+1;
			for( int j = 0; j < gugu[i].length; j++) {
			j=j+1;
				System.out.print( gugu[i][j] + " ");
			}
		System.out.println();
		}

	}

}
